{
  "name": "ufp-optimizer",
  "version": "0.10.0",
  "description": "optimizing ufp frontend packages",
  "main": "src/UfpOptimizer.js",
  "scripts": {
    "unit-test": "cross-env NODE_ENV=test NODE_PATH=. mocha --opts build/mocha.opts",
    "e2e-test": "cross-env NODE_ENV=test NODE_PATH=. mocha --opts build/mocha-e2e.opts",
    "test": "npm run build-0 && npm run build-1 && npm run build-2 && npm run build-3 && npm run build-4 && npm run build-5",
    "build-0": "node bin/ufp-optimizer-cli.js optimize examples/0/fullpage dist/dist-0",
    "build-1": "node bin/ufp-optimizer-cli.js optimize examples/1/fast dist/dist-1",
    "build-2": "node examples/2/test-fast.js",
    "build-3": "node examples/3/test-same-dir.js",
    "build-4": "node bin/ufp-optimizer-cli.js optimize examples/4/broken dist/dist-4",
    "build-5": "node bin/ufp-optimizer-cli.js optimize examples/5/not-existing dist/dist-5",
    "eslint": "./node_modules/.bin/eslint src/ bin/ tests/e2e-tests/*.js tests/e2e-tests/tests/*.js tests/src/--fix"
  },
  "engines": {
    "node": ">=8.5"
  },
  "bin": {
    "ufp-optimizer-cli": "bin/ufp-optimizer-cli.js"
  },
  "keywords": "compress brotli zopfli optimization",
  "author": "Frontend Solutions GmbH",
  "license": "MIT",
  "repository": "https://github.com/FrontendSolutionsGmbH/ufp-optimizer.git",
  "dependencies": {
    "brotli": "^1.3.2",
    "clean-css": "^4.1.9",
    "concurrently": "^3.1.0",
    "console.table": "^0.9.1",
    "fs-extra": "^1.0.0",
    "html-minifier": "^3.5.5",
    "imagemin": "^5.2.2",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-jpeg-recompress": "^5.1.0",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngcrush": "^5.0.0",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.2.0",
    "imagemin-webp": "^4.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.defaultsdeep": "^4.6.0",
    "node-gyp": "^3.6.2",
    "node-zopfli": "^2.0.2",
    "uglify-js": "^3.1.3",
    "winston": "^2.4.0",
    "yargs": "^9.0.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-config-volox": "^2.1.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-flowtype": "^2.37.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.9.1",
    "eslint-plugin-jsx": "^0.0.2",
    "eslint-plugin-new-with-error": "^1.1.0",
    "eslint-plugin-no-inferred-method-name": "^1.0.2",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-redux": "^0.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.5.3",
    "mocha-junit-reporter": "^1.15.0",
    "mocha-multi-reporters": "^1.1.6"
  }
}
