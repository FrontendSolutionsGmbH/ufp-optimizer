{
  "name": "ufp-optimizer",
  "version": "0.11.0",
  "description": "optimizing ufp frontend packages",
  "main": "src/UfpOptimizer.js",
  "browserslist": [
    "> 1%",
    "ie 10"
  ],
  "scripts": {
    "unit-tests": "cross-env NODE_ENV=test NODE_PATH=. mocha --opts tests/build/mocha.opts",
    "e2e-tests": "cross-env NODE_ENV=test NODE_PATH=. mocha --opts tests/build/mocha-e2e.opts",
    "test": "npm run build-0 && npm run build-1 && npm run build-2 && npm run build-3 && npm run build-4 && npm run build-5",
    "build-0": "node bin/ufp-optimizer-cli.js optimize examples/0/fullpage dist/dist-0",
    "build-1": "node bin/ufp-optimizer-cli.js optimize examples/1/fast dist/dist-1",
    "build-2": "node examples/2/test-fast.js",
    "build-3": "node examples/3/test-same-dir.js",
    "build-4": "node bin/ufp-optimizer-cli.js optimize examples/4/broken dist/dist-4",
    "build-5": "node bin/ufp-optimizer-cli.js optimize examples/5/not-existing dist/dist-5",
    "eslint": "./node_modules/.bin/eslint src/ bin/ tests/e2e-tests/*.js tests/e2e-tests/tests/*.js tests/src/ --fix"
  },
  "engines": {
    "node": ">=8.5"
  },
  "bin": {
    "ufp-optimizer-cli": "bin/ufp-optimizer-cli.js"
  },
  "keywords": [
    "compress",
    "brotli",
    "zopfli",
    "optimization"
  ],
  "author": "Frontend Solutions GmbH",
  "license": "MIT",
  "repository": "https://github.com/FrontendSolutionsGmbH/ufp-optimizer.git",
  "dependencies": {
    "autoprefixer": "9.5.1",
    "brotli": "1.3.2",
    "concurrently": "4.1.0",
    "console.table": "0.10.0",
    "fs-extra": "7.0.1",
    "html-minifier": "4.0.0",
    "imagemin": "6.1.0",
    "imagemin-giflossy": "5.1.10",
    "imagemin-jpeg-recompress": "6.0.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngcrush": "6.0.0",
    "imagemin-pngquant": "7.0.0",
    "imagemin-svgo": "7.0.0",
    "imagemin-webp": "5.0.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.defaultsdeep": "4.6.0",
    "node-gyp": "4.0.0",
    "node-zopfli": "2.0.3",
    "postcss": "7.0.14",
    "postcss-clean": "1.1.0",
    "postcss-preset-env": "6.6.0",
    "postcss-grid": "2.0.0",
    "uglify-js": "3.5.10",
    "walk-sync": "^1.1.3",
    "winston": "3.2.1",
    "yargs": "13.2.2",
    "zlib": "1.0.5"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-config-volox": "2.1.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-chai": "0.0.1",
    "eslint-plugin-chai-friendly": "0.4.1",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-flowtype": "3.6.1",
    "eslint-plugin-immutable": "1.0.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-new-with-error": "2.0.0",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-no-use-extend-native": "0.4.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-redux": "0.1.0",
    "eslint-plugin-standard": "4.0.0",
    "mocha": "6.1.4",
    "mocha-junit-reporter": "1.22.0",
    "mocha-multi-reporters": "1.1.7"
  }
}
